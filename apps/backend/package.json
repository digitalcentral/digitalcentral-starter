{
	"name": "@digitalcentral/backend",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "pnpm with-env convex dev",
		"release": "pnpm with-env convex deploy -y",
		"auth:generate": "pnpm dlx @better-auth/cli generate --config ./convex/betterAuth/auth.ts --output ./convex/betterAuth/GeneratedSchema.ts",
		"with-env": "dotenv -e ../../.env.local --"
	},
	"dependencies": {
		"@convex-dev/better-auth": "^0.10.10",
		"better-auth": "^1.4.18",
		"convex": "^1.31.7"
	},
	"devDependencies": {
		"dotenv-cli": "^10.0.0"
	},
	"exports": {
		"./convex/_generated/api": {
			"types": "./convex/_generated/api.d.ts",
			"import": "./convex/_generated/api.js",
			"default": "./convex/_generated/api.js"
		},
		"./convex/_generated/dataModel": {
			"types": "./convex/_generated/dataModel.d.ts",
			"default": "./convex/_generated/dataModel.d.ts"
		}
	}
}
